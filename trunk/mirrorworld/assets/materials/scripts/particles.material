material Particles/SpinBeam
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			scene_blend add

			texture_unit
			{
				texture spinbeam.png
				tex_address_mode clamp
			}
		}
	}
}

material Particles/explosion
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			scene_blend add

			texture_unit
			{
				texture explosion.png
			}
		}
	}
}
